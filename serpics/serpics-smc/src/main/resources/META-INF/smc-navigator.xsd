<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<xsd:schema
xmlns="http://www.serpicsframework.com/schema/smc-navigator" 
xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"
targetNamespace="http://www.serpicsframework.com/schema/smc-navigator">

  <xsd:element name="navigator-definition" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
     <xsd:complexType>
      <xsd:choice minOccurs="1" maxOccurs="1">
        <xsd:element name="navigator" maxOccurs="1" minOccurs="1">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="navigator-item" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:attribute name="allow-child" type="xsd:boolean" default="false" />
                  <xsd:attribute name="bean" type="xsd:string" />
                  <xsd:attribute name="class" type="xsd:string" />
                  <xsd:attribute name="name" type="xsd:string" use="required" />
                  <xsd:attribute name="parent" type="xsd:string" default="" />
                  <xsd:attribute name="roles" type="xsd:string" default="" />
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="name" type="xsd:string" />
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
 </xsd:element>
 </xsd:schema>